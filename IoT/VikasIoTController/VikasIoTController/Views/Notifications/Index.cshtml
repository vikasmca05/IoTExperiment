@model VikasIoTController.Models.NotificationViewModel
@{
    ViewBag.Title = "Notifications Page";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>

<body>
@using (Html.BeginForm("SendToastMessage", "Notifications", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    <div class="form-group">
        @Html.LabelFor(m => m.Pushmessage, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.Pushmessage, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
            @Html.LabelFor(m => m.UserTag, new { @class = "col-md-2 control-label" })
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.UserTag, new { @class = "form-control" })
            </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="SendToastMessage" />
        </div>
    </div>
}
    <div>
@*                   @Html.ActionLink("SendNotification", "SendToastMessage")</li>*@
       
        
    </div>
</body>
</html>
